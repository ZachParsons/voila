<h1>Apprentice Registration</h1>

<%= link_to "Master Registration", new_master_path %>

<%= form_for(:user, url: users_path) do |f| %>
  <%= f.label :stage_name, "Stage Name" %>
  <%= f.text_field :stage_name %>

  <%= f.label :email, "Email" %>
  <%= f.text_field :email %>

  <%= f.label :password, "Password" %>
  <%= f.text_field :password %>

  <%= f.submit %>
<% end %>
